{
  "sourceMaps": true,
  "presets": ["es2015", "react", "flow"],
  "plugins": [
    "transform-class-properties",
    "transform-html-import-to-string",
    "transform-object-rest-spread"
  ],
  "env": {
    "grafana": {
      "plugins": [
        "transform-es2015-modules-systemjs",
        "transform-class-properties",
        "transform-html-import-to-string",
        "transform-object-rest-spread"
      ]
    },
    "test": {
      "sourceMaps": false
    }
  }
}
